                            android:textColor="@color/colorAccent"
                            android:textSize="@dimen/titleSize"
                            android:visibility="gone" />
                    </FrameLayout>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_address_profile"
                        android:id="@+id/address_image"
                        android:layout_below="@id/email"
                        android:layout_marginTop="7dp"
                        android:layout_marginEnd="8dp"/>

                    <net.phpsm.simsim.simsiminstantorder.CustomViews.CustomTextView
                        android:id="@+id/location"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/email"
                        android:layout_toEndOf="@id/address_image"
                        android:layout_marginTop="10dp"
                        android:text="@string/location"
                        android:textColor="@color/bold_name"
                        android:textSize="@dimen/titleSize"
                        android:textStyle="bold" />

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/email"

