                        android:textColor="@color/bold_name"
                        android:textSize="@dimen/boldtext1"
                        android:textStyle="bold"
                        android:visibility="visible"/>

                    <net.phpsm.simsim.simsiminstantorder.CustomViews.CustomEditText
