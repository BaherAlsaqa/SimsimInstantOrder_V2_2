                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/notifitxt"
                        android:layout_marginTop="@dimen/activity_horizontal_margin"
                        android:id="@+id/lang"
                        android:text="@string/lang"
                        android:layout_marginBottom="@dimen/activity_horizontal_margin"
                        android:textColor="@color/bold_name"
                        android:textSize="@dimen/titleSize">
                    </net.phpsm.simsim.simsiminstantorder.CustomViews.CustomTextView>


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/line2"
                    android:layout_margin="20dp"
                    android:layout_marginTop="10dp"
                    android:visibility="gone">

                    <net.phpsm.simsim.simsiminstantorder.CustomViews.CustomTextView
                        android:id="@+id/lastr"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:text="@string/lastrestaurant"
                        android:textColor="@color/bold_name"
                        android:textSize="@dimen/titleSize"
                        android:textStyle="bold" />

                    <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/lastrestimage"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="2dp"
                        android:layout_toEndOf="@id/lastr"
                        android:src="@drawable/place_holder_provider"
                        android:textSize="@dimen/titleSize"
                        app:civ_border_color="@color/signup"
                        app:civ_border_overlay="true"
                        app:civ_border_width="1dp" />

                    <net.phpsm.simsim.simsiminstantorder.CustomViews.CustomTextView
                        android:id="@+id/res_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="5dp"
                        android:layout_toEndOf="@id/lastrestimage"
                        android:text=""
                        android:textColor="@color/bold_name"
                        android:textSize="@dimen/titleSize" />

                    <net.phpsm.simsim.simsiminstantorder.CustomViews.CustomTextView
                        android:id="@+id/lastorder"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/lastr"
                        android:layout_marginTop="13dp"
                        android:text="@string/lastorder"
                        android:textColor="@color/bold_name"
                        android:textSize="@dimen/titleSize"
                        android:textStyle="bold" />

                    <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/lastOrderimage"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_below="@id/lastr"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="8dp"
                        android:layout_toEndOf="@id/lastorder"
                        android:src="@drawable/place_holder_provider"
                        app:civ_border_color="@color/signup"
                        app:civ_border_overlay="true"
                        app:civ_border_width="1dp" />

                    <net.phpsm.simsim.simsiminstantorder.CustomViews.CustomTextView
                        android:id="@+id/ordervalue"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/lastr"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="13dp"
                        android:layout_toEndOf="@id/lastOrderimage"
                        android:text=""
                        android:textColor="@color/bold_name"
                        android:textSize="@dimen/titleSize" />

                </RelativeLayout>

            </RelativeLayout>

            <!--<ProgressBar
                android:id="@+id/progress"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:visibility="gone" />-->
            <com.wang.avi.AVLoadingIndicatorView
                android:id="@+id/progress"
                style="@style/AVLoadingIndicatorView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                app:indicatorColor="@color/colorPrimary"
                app:indicatorName="BallPulseIndicator"
                android:layout_gravity="center" />
        </FrameLayout>

        <net.phpsm.simsim.simsiminstantorder.CustomViews.CustomButton
            android:id="@+id/btnupdate"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            style="?android:attr/borderlessButtonStyle"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/shapbtnlogin"
            android:gravity="center"
            android:text="@string/editprofile"
            android:textColor="@color/white"
            android:textSize="@dimen/content"
            android:visibility="gone"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="25dp"/>



    </LinearLayout>
</ScrollView>
</android.support.v4.widget.SwipeRefreshLayout>