                            android:layout_height="25dp"
                            android:layout_marginEnd="5dp"
                            android:button="@android:color/transparent"
                            android:layout_marginStart="5dp"
                            android:background="@drawable/van_selector" />

                    </RadioGroup>

                    <ImageView
                        android:id="@+id/carimage"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_below="@id/location"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="25dp"
                        android:layout_toEndOf="@id/car"
                        android:visibility="visible" />

                    <net.phpsm.simsim.simsiminstantorder.CustomViews.CustomTextView
                        android:id="@+id/carname"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/location"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="25dp"
                        android:layout_toEndOf="@id/carimage"
                        android:text=""
                        android:textColor="@color/colorAccent"
                        android:textSize="@dimen/titleSize"
                        android:visibility="visible" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_color_profile"
